# word_sets v2
# module for use with japanese_quiz_generator; classes Quiz and Word initialized, vocabulary sets established (plus a few extras).
# new vocabulary sets can be easily added. Instructions for that at the end of 'word_sets.py' (current file).

engparsing_list = []
japarsing_list = []

extra = 0

class Word:
	def __init__(word, english, romaji, kana, kanji, engparsing, japarsing):
		global engparsing_list
		word.english = english
		word.romaji = romaji
		word.kana = kana
		word.kanji = kanji
		if kanji == None:
			word.kanji = kana
		if engparsing == None:
			word.engparsing = english
		else:
			word.engparsing = engparsing
			engparsing_list.append(engparsing)
			if "(alt)" in english:
				engparsing_list.append(english.replace("(alt)", "alt"))
				engparsing_list.append(english.replace("(alt)", ""))
			word.engparsing = engparsing_list
		if japarsing == None:
			word.japarsing = romaji
		else:
			word.japarsing = japarsing
			japarsing_list.append(japarsing)
			word.japarsing = japarsing_list
			
class Quiz:
	def __init__(quiz, qn, wl, extra):
		quiz.qn = qn
		quiz.wl = ['w1', 'w2', 'w3', 'w4', 'w5', 'w6', 'w7', 'w8', 'w9', 'w10']
		if extra > 0:
			for i in range(extra):
				quiz.wl.append(('w' + '1' + str(extra)))

